import pandas as pd
from sklearn.model_selection import train_test_split
import pickle
from sklearn.metrics import accuracy_score
original_dataset = pd.read_csv('CAR DETAILS.csv')
selected_data = original_dataset.sample(n=20, random_state=42)
saved_model = pickle.load(open('dt_classifier.pkl', 'rb'))
features = selected_data.drop('selling_price', axis=1)
predictions = saved_model.predict(features)
target = selected_data['selling_price']  
accuracy = accuracy_score(target, predictions)
print("Model Accuracy:", accuracy)